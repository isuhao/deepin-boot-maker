Dtk--Widget--DButtonList {
    background-color: #1A1B1B;
    border: none;
    outline: none;
}

Dtk--Widget--DButtonList::item {
    border: none;
    outline: none;
}

Dtk--Widget--DButtonList QPushButton:hover{
    color: white;
}

Dtk--Widget--DButtonList QFrame#BorderFrame{
  background-color: transparent;
}

Dtk--Widget--DButtonList QPushButton{
    background-color: transparent;
    color: #b4b4b4;
    border: none;
    border-radius:3px;
    text-align: left center;
    padding-left: 40px;
}

Dtk--Widget--DButtonList QPushButton:checked{
    background-color: #66000000;
    color: #2ca7f8;
}

Dtk--Widget--DButtonList QPushButton:checked[state="OnlyOne"]{
    background-color: transparent;
    color:#01BDFF;
}

Dtk--Widget--DButtonList QPushButton:checked[state="normal"]{
    background-color: lightblack;
    color:#01BDFF;
}
